<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

    <bean id="mpPublication" class="gov.usgs.cida.pubs.domain.MpPublication" autowire="byName"/>
    <bean id="mpPublicationContributor" class="gov.usgs.cida.pubs.domain.MpPublicationContributor" autowire="byName"/>


    <bean id="contributor" class="gov.usgs.cida.pubs.domain.Contributor" autowire="byName"/>
    <bean id="contributorType" class="gov.usgs.cida.pubs.domain.ContributorType" autowire="byName"/>
    <bean id="costCenter" class="gov.usgs.cida.pubs.domain.CostCenter" autowire="byName"/>
    <bean id="publicationType" class="gov.usgs.cida.pubs.domain.PublicationType" autowire="byName"/>
    <bean id="publicationSubtype" class="gov.usgs.cida.pubs.domain.PublicationSubtype" autowire="byName"/>
    <bean id="publicationSeries" class="gov.usgs.cida.pubs.domain.PublicationSeries" autowire="byName"/>

</beans>