<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

    <import resource="testJndiContext.xml"/>
    <import resource="applicationContext.xml"/>

    <bean id="expectedGetMpPub1" class="java.lang.String">
        <constructor-arg>
            <bean class="org.springframework.util.FileCopyUtils" factory-method="copyToByteArray">
                <constructor-arg value="classpath:testData/getMpPub1.json" type="java.io.InputStream" />
            </bean>
        </constructor-arg>
    </bean>

    <bean id="contributorsXml" class="java.lang.String">
        <constructor-arg>
            <bean class="org.springframework.util.FileCopyUtils" factory-method="copyToByteArray">
                <constructor-arg value="classpath:testData/contributors.xml" type="java.io.InputStream" />
            </bean>
        </constructor-arg>
    </bean>

    <bean id="usgsContributorXml" class="java.lang.String">
        <constructor-arg>
            <bean class="org.springframework.util.FileCopyUtils" factory-method="copyToByteArray">
                <constructor-arg value="classpath:testData/usgsContributor.xml" type="java.io.InputStream" />
            </bean>
        </constructor-arg>
    </bean>

    <bean id="newOutsideContributorXml" class="java.lang.String">
        <constructor-arg>
            <bean class="org.springframework.util.FileCopyUtils" factory-method="copyToByteArray">
                <constructor-arg value="classpath:testData/newOutsideContributor.xml" type="java.io.InputStream" />
            </bean>
        </constructor-arg>
    </bean>

    <bean id="existingOutsideContributorXml" class="java.lang.String">
        <constructor-arg>
            <bean class="org.springframework.util.FileCopyUtils" factory-method="copyToByteArray">
                <constructor-arg value="classpath:testData/existingOutsideContributor.xml" type="java.io.InputStream" />
            </bean>
        </constructor-arg>
    </bean>

    <bean id="costCenterXml" class="java.lang.String">
        <constructor-arg>
            <bean class="org.springframework.util.FileCopyUtils" factory-method="copyToByteArray">
                <constructor-arg value="classpath:testData/costCenter.xml" type="java.io.InputStream" />
            </bean>
        </constructor-arg>
    </bean>

    <bean id="feedXml" class="java.lang.String">
        <constructor-arg>
            <bean class="org.springframework.util.FileCopyUtils" factory-method="copyToByteArray">
                <constructor-arg value="classpath:testData/feed.xml" type="java.io.InputStream" />
            </bean>
        </constructor-arg>
    </bean>

    <bean id="notesXml" class="java.lang.String">
        <constructor-arg>
            <bean class="org.springframework.util.FileCopyUtils" factory-method="copyToByteArray">
                <constructor-arg value="classpath:testData/notes.xml" type="java.io.InputStream" />
            </bean>
        </constructor-arg>
    </bean>

	<import resource="classpath:/spring/testSecurityContext.xml"/>
</beans>
